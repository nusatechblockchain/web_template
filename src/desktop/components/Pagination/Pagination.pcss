.pagination-component {
    p {
        color: var(--text-secondary-color);
        padding: 12px 12px 0 12px;
    }
    button {
        background: var(--body-background-color);
        border: 1px solid var(--border-color);
        width: 36px;
        height: 36px;
        border-radius: 100px;
        margin: 0 6px;
    }
}
