.pg-market-order {
    .navbar-brand {
        font-weight: 700;
        font-size: 20px;
        line-height: 23px;
        color: var(--text-secondary-color);
    }

    .menu-snackbar.active {
        color: var(--text-secondary-color);
    }

    .menu-snackbar {
        font-weight: 500;
        font-size: 10px;
        line-height: 16px;
        color: var(--text-grey-color);
    }

    .head-container {
        margin: 24px 0;
    }

    .heading-one {
        font-weight: 700;
        font-size: 20px;
        line-height: 23px;
        color: var(--text-secondary-color);
    }

    .head-action {
        gap: 10px;
    }

    .head-action img {
        cursor: pointer;
    }

    .nav-tabs {
        border-bottom: none;
        gap: 16px;
    }

    a.nav-link,
    .nav-tabs {
        font-weight: 600;
        font-size: 14px;
        line-height: 16px;
        color: var(--text-grey-color);
        background: transparent;
        border: none;
        padding: 8px 0;
    }

    .nav-tabs .nav-link:hover {
        border-color: transparent;
    }

    .nav-tabs .nav-item.show .nav-link,
    .nav-tabs .nav-link.active {
        font-weight: 600;
        font-size: 14px;
        line-height: 16px;
        text-align: center;
        background: var(--text-gradient-color) !important;
        -webkit-background-clip: text !important;
        -webkit-text-fill-color: transparent !important;
        padding: 8px 0;
        border-bottom: 1px solid var(--text-contrast-color) !important;
    }

    .tab-content {
        width: 100%;
    }

    .cancel-all-container {
        gap: 6px;
        cursor: pointer;
    }

    .cancel-all-container p {
        font-weight: 600;
        font-size: 14px;
        line-height: 16px;
        color: var(--text-grey-color);
    }

    .table-order-container table {
        width: 100%;
        padding: 0;
        margin: 0;
        margin-bottom: 60%;
    }

    .table-order-container thead {
        background: transparent !important;
    }

    .table-order-container th,
    .table-order-container td {
        padding: 12px 0;
    }

    .name-vol-container {
        gap: 8px;
    }

    .name-col {
        gap: 4px;
    }

    .name-col p {
        margin: 0;
        font-weight: 400;
        font-size: 14px;
        line-height: 16px;
        color: var(--text-grey-color);
    }

    .td-amount {
        gap: 2px;
    }

    .td-amount h3 {
        font-weight: 600;
        font-size: 14px;
        line-height: 16px;
        color: var(--text-secondary-color);
    }

    .td-amount p {
        font-weight: 500;
        font-size: 10px;
        line-height: 16px;
        color: var(--text-grey-color);
    }

    .td-price {
        font-weight: 400;
        font-size: 14px;
        line-height: 16px;
        color: var(--text-secondary-color);
    }

    .td-type {
        font-weight: 400;
        font-size: 14px;
        line-height: 16px;
        background: var(--text-gradient-color);
        -webkit-background-clip: text;
        -webkit-text-fill-color: transparent;
        background-clip: text;
        text-fill-color: transparent;
    }

    .td-status {
        font-weight: 400;
        font-size: 14px;
        line-height: 16px;
        color: var(--text-danger-color);
        cursor: pointer;
    }

    .td-status-done {
        font-weight: 400;
        font-size: 14px;
        line-height: 16px;
        color: var(--text-grey-color);
    }

    /* Off Canvas Cancel Start */
    .off-canvas {
        max-width: 768px;
        width: 100%;
        height: 100%;
        background: var(--modal-background-color);
        z-index: 10000;
        top: 0;
        left: 0;
        right: 0;
        transform: translateY(100%);
        transition: 0.5s;

        &.show {
            transform: translateY(0%);
        }
    }

    .off-canvas-content-container {
        background: var(--main-background-color);
        height: 80%;
        width: 100%;
        max-width: 768px;
        margin: 0 auto;
        z-index: 20000 !important;
        border-radius: 8px 8px 0 0;
        padding: 24px;
    }

    .off-canvas-content-head {
        gap: 8px;
        margin-bottom: 18px;
    }

    .off-canvas-content-head h3 {
        font-weight: 700;
        font-size: 20px;
        line-height: 23px;
        color: var(--text-secondary-color);
    }

    .off-canvas-content-head span {
        font-weight: 400;
        font-size: 14px;
        line-height: 16px;
        color: var(--text-secondary-color);
    }

    table .table-canvas {
        margin-bottom: 38px;
    }

    .table-canvas tr {
        padding: 16px 10px;
        border-bottom: 0.5px solid var(--text-grey-color);
    }

    .td-title {
        font-weight: 700;
        font-size: 16px;
        line-height: 19px;
        color: var(--text-grey-color);
    }

    .td-value {
        font-weight: 700;
        font-size: 16px;
        line-height: 19px;
        color: var(--text-primary-color);
    }
    /* Off Canvas Cancel End */

